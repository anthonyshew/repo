{
	"name": "shewrepo",
	"version": "0.1.0",
	"scripts": {
		"build": "turbo run build",
		"clean": "find . -name node_modules -o -name .next -o -name dist -o -name .contentlayer -type d -prune | xargs rm -rf",
		"dev": "turbo run dev //#dev:ts",
		"dev:ts": "tsc -b -w --preserveWatchOutput",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"check-types": "turbo run check-types --continue",
		"knip": "knip"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/node": "^24.0.10",
		"knip": "^5.61.3",
		"turbo": "^2.5.4",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@10.12.4",
	"engines": {
		"node": ">=22"
	}
}
