{
	"tasks": {
		"dev": {
			"persistent": true,
			"cache": false,
			"with": ["//#dev:ts"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"//#knip": {},
		"//#lint": {},
		"//#lint:fix": {
			"cache": false
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"outputs": ["dist"]
		},
		"//#dev:ts": {
			"cache": false,
			"persistent": true
		}
	}
}
